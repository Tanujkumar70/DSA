#include <iostream>

using namespace std;

int main()
{
   int n,m;
   cin>>n>>m;
  int count=n;
   int arr1[n];
   int arr2[m];
   for(int i=0;i<n;i++){
       cin>>arr1[i];
   }
   for(int i=0;i<m;i++){
       cin>>arr2[i];
   }
for(int i=0;i<m;i++){
    int j;
    for( j=0;j<n;j++){
        if(arr2[i]=arr1[j]){
            break;
        }
    }
if(j==n){
    count++;
}
    
}
cout<<count;
    return 0;
}
