#include<bits/stdc++.h>
using namespace std;
int main()
{
string str;
int i,j;
cin>>str;
sort(str.begin(),str.end());
for(int i=0;i<str.size();i++){
   int count=1;
    char ch=str[i];
       for(int j=i+1;j<str.size();j++)
    {
    char c=str[j];
     if(c!=ch){
        break;
    }
    else{
        count++;
    }
    } 
    if(count !=1){
        cout<<ch<<count;
}

       //i=j-1;
    }
return 0;
}
