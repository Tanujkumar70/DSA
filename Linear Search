#include <iostream>

using namespace std;

int main()
{
int n;
cin>>n;
int a[n];
for(int i=0;i<n;i++){
    cin>>a[i];
} 

int key;
cin>>key;
int i;
for(int i=0;i<n;i++){
    if(key==a[i]){
       
        std::cout << "found at index"<< i<< std::endl;
         break;
        
    }
} 

if(i==n){
    cout<<key;
}

    return 0;
}
 
